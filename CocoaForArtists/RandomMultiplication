#import "CFACanvas.h"
@implementation CFACanvas
-(void)setup {
	CFALog(@"A Bit of *");
	//Drag or keypresses work
	[self drawStyle:EVENTBASED];
}

-(void)draw {
	int A = [CFAMath randomInt:1000];
	int B = [CFAMath randomInt:1000];
	CFALog(@"A = %d",A);
	CFALog(@"B = %d",B);
	CFALog(@"A * B = %d", A*B);
}
@end